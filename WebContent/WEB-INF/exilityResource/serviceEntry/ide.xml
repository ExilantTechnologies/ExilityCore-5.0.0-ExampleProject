<serviceList xmlns="http://com.exilant.exility/schema" >
<!-- 
    All services used by IDE
-->
    <serviceEntries>
    	<serviceEntry name="generateLabels"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="none"
                 description="Rebuild translation table for labels."
                 serviceName="generateLabels"
                 fullyQualifiedClassName="com.exilant.exility.ide.GetLabelsFromDictionary"
                 />
    	<serviceEntry name="getLanguages"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="none"
                 description="get list of languages this project has been set-up to deal with."
                 fullyQualifiedClassName="com.exilant.exility.ide.GetLanguages"
                 />
      <serviceEntry name="generateDataTypes"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="none"
                 description="Generate Data Types.xml and dataTypes.xsd"
                 fullyQualifiedClassName="com.exilant.exility.ide.GenerateDataTypes"
                 />
      <serviceEntry name="generatePage"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="none"
                 description="Generates page"
                 fullyQualifiedClassName="com.exilant.exility.ide.GeneratePage"
                 />

      <serviceEntry name="saveSql"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="none"
                 description="Save details of a Sql"
                 fullyQualifiedClassName="com.exilant.exility.ide.SaveSql"
                 />
      <serviceEntry name="saveAp"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="none"
                 description="Generates page"
                 fullyQualifiedClassName="com.exilant.exility.core.SaveAp"
                 />
      <serviceEntry name="saveTable"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="none"
                 description="Save details of a table"
                 fullyQualifiedClassName="com.exilant.exility.core.SaveTable"
                 />

      <serviceEntry name="getVersion"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="none"
                 description="Get Exility server version"
                 fullyQualifiedClassName="com.exilant.exility.ide.GetVersion"
                 />

      <serviceEntry name="dbToTable"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="readOnly"
                 description="Get a default table definition based on the table defined in the data base"
                 fullyQualifiedClassName="com.exilant.exility.ide.DbToTable"
                 />

      <batchEntry name="getAllResourceLists"
      				batchServiceName="internal.dummyLogin"
      				inputGridName="someTable"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="readOnly"
                 description="Get lists of all resources"
                 fullyQualifiedClassName="com.exilant.exility.ide.GetAllResourceLists"
                 />

      <serviceEntry name="temp"
                 allowAllInput="true"
                 allowAllOutput="true"
                 dataAccessType="none"
                 description="temp"
                 fullyQualifiedClassName="com.exilant.exility.ide.Temp"
                 />

    </serviceEntries>
    
</serviceList>
