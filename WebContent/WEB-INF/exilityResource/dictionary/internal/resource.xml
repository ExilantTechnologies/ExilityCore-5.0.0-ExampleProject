        <dataGroup name="resource"
                   label="Resource"
                   businessDescription="Used by pages that manage Exility resources."
                   >
            <elements>
                <dataElement name="resourceType"
                             dataType="entityName"
                             label="Resource type"
                             businessDescription="One of valid resource types like service,page etc.."
                             />
                <dataElement name="resourceName"
                             dataType="entityName"
                             businessDescription="true"
                             label="Name"
                             />
                <dataElement name="module"
                             dataType="entityName"
                             businessDescription="true"
                             label="Module"
                             />
                <dataElement name="subModule"
                             dataType="entityName"
                             businessDescription="true"
                             label="Sub module"
                             />
                <dataElement name="name"
                             dataType="entityName"
                             label="Name"
                             businessDescription="name should be used only as a dataElement, and not directly used as a name of an element. 'name' is not a good name. Any name shoudl be qualified."
                             />
                <dataElement name="folderName"
                             dataType="entityName"
                             businessDescription="true"
                             label="Folder name"
                             />
                <dataElement name="nbrGenerated"
                             dataType="number"
                             businessDescription="true"
                             label="Nbr pages generated"
                             />

                <dataElement name="toRecurse"
                             dataType="boolean"
                             businessDescription="true"
                             label="All refrred resources?"
                             />
                <dataElement name="saveAction"
                             dataType="entityName"
                             businessDescription="true"
                             label="Action"
                             />
                <dataElement name="_type"
                             dataType="entityName"
                             businessDescription="true"
                             label="Type"
                             />

            </elements>
        </dataGroup>
    </groups>
</dataDictionary>