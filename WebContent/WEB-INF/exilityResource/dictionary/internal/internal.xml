    <groups>
        <dataGroup name="internal"
                   label="Internal"
                   businessDescription="Used internally by the project. No meaning to business">
            <elements>
                <dataElement name="id"
                             dataType="id"
                             label="Id"
                             />
              <dataElement name="inactive"
                           dataType="boolean"
                           label="Is Active?"
                           valueList="0,Yes;1,No"
                           businessDescription="This field label is inverted to make it meaningful to user. inactive is a negative word, but used internally to get false as a default."
                           />
                <dataElement  name="bulkAction"
                              dataType="text"
                              label="Bulk Action "
                              />
                <dataElement name="actionField"
                             dataType="entityName"
                             label="Bulk action"
                             businessDescription="Used by grid panel to mark the operation to be carried out on the row. It is internally managed,and will not be visible to users any where"
                             />
                <dataElement name="filterField"
                             dataType="entityName"
                             label="Filter"
                             businessDescription="Field that is used to get the value using which a field can be matched to filter rows"
                             />
                <dataElement  name="fileName"
                              dataType="entityName"         	       
                              label="File Name"     
                              />
                <dataElement  name="folderName"
                              dataType="entityName"
                              label="Folder Name"
                              />
                <dataElement name="liveWithErrors"
                             dataType="boolean"
                             label="Do you want to live with errors in the project"
                             />
              <dataElement name="date"
                           businessDescription="Generic one for any temp"
                           dataType="date"
                           label="Date"
                             />
              <dataElement name="yesNo"
                           businessDescription="Generic one for any temp"
                           dataType="yesNo"
                           label="What do you say?"
                             />
              <dataElement name="dateTime"
                           businessDescription="generic date with time field"
                           dataType="dateTime"
                           label="Date with Time"
                           messageName="specialMessage"
                           />

            </elements>
        </dataGroup>
    </groups>
</dataDictionary>